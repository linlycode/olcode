{
	"extends": ["airbnb", "prettier"],
	"env": {
		"browser": true
	},
	"parser": "babel-eslint",
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "error",
		"no-tabs": 0,
		"indent": [
			"error",
			"tab",
			{"SwitchCase": 1}
		],
		"semi": [
			"error",
			"never"
		],
		"react/jsx-filename-extension": "never",
		"react/jsx-indent": [true, "tab"],
		"react/jsx-indent-props": [true, "tab"]
	}
}